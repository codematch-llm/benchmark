{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
      "json": {
        "()": "mylogger.MyJSONFormatter",
        "fmt_keys": {
          "level": "levelname",
          "message": "message",
          "timestamp": "timestamp",
          "logger": "name",
          "module": "module",
          "function": "funcName",
          "line": "lineno"
        }
      },
      "simple": {
        "format": "%(asctime)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "simple"
      },
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "INFO",
        "formatter": "json",
        "filename": "logs/benchmark.log",
        "maxBytes": 10485760,
        "backupCount": 3
      }
    },
    "root": {
      "level": "INFO",
      "handlers": ["console", "file"]
    }
  }
  